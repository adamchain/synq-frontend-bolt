{
  "name": "synq",
  "private": true,
  "version": "1.0.0",
  "description": "SYNQ Express Server & React App",
  "scripts": {
    "test-app": "concurrently -n=\"server,client\" -c=\"green,cyan\"  \"npm:start:server:test\" \"npm:start:client:dev\"",
    "start:server:test": "cross-env NODE_ENV=test nodemon --ignore 'src' --ignore 'cypress' --exec \"node server/index.mjs\"",
    "start:server:test:ci": "cross-env NODE_ENV=test node server/index.mjs",
    "start:client:dev": "cross-env NODE_ENV=development webpack serve --port 3000",
    "dev": "cross-env NODE_ENV=dev DEV=1 nodemon --ignore 'src' --exec \"node server/index.mjs\"",
    "local": "cross-env NODE_ENV=local nodemon --ignore 'src' --exec \"node server/index.mjs\"",
    "watch": "webpack --watch",
    "build": "cross-env NODE_ENV=production webpack",
    "lint": "eslint ./src --ext .jsx --ext .js",
    "lint:fix": "eslint ./src --ext .jsx --ext .js --fix",
    "test": "NODE_ENV=test jest --runInBand --forceExit",
    "prepare": "husky install",
    "test:watch": "jest --watch **/*.spec.js",
    "test:coverage": "jest --coverage **/*.spec.js",
    "format": "npx prettier --write .",
    "cypress:open": "cypress open",
    "start": "node server/index.mjs",
    "web": "npm run start:client:dev",
    "server": "npm run start:server:test",
    "build:schema": "sequelize-auto -h synq-db-uat.c9s22uqoi5rc.us-east-2.rds.amazonaws.com -d whskr_migration -u admin -x -p 3306 --dialect mysql -o ./server/models --cm p --cf p --cp c -l esm"
  },
  "main": "index.js",
  "engines": {
    "node": "^20.18.3",
    "npm": "^10.8.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [],
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WHSKRECHNOLOGIES/snyq.git"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "axios": "^1.9.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "bcrypt": "^6.0.0",
    "bluebird": "^3.7.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.3",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "express-rest-error": "^1.2.1",
    "express-user-auth": "^1.4.0",
    "is-valid-email": "^0.0.5",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.1",
    "mysql2": "^3.14.3",
    "nodemailer": "^6.7.2",
    "parse-bearer-token": "^1.0.1",
    "pdf-lib": "^1.17.1",
    "sequelize": "^6.14.1",
    "sequelize-auto": "^0.8.8",
    "swr": "^1.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.17.12",
    "@databases/pg-migrations": "^5.0.0",
    "@databases/pg-test": "^3.1.1",
    "@jest/globals": "^27.4.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.0",
    "autoprefixer": "^10.4.7",
    "ava": "^4.0.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "buffer": "^6.0.3",
    "compression-webpack-plugin": "^10.0.0",
    "concurrently": "^7.4.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.10",
    "cypress": "^10.11.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.4.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "mini-css-extract-plugin": "^2.6.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2"
  }
}
